{
	"compilerOptions": {
		"target": "ES2022",
		"module": "commonjs",
		"lib": [
			"ES2022"
		],
		"outDir": "./dist",
		"rootDir": ".",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"declaration": false
	},
	"include": [
		"*.ts"
	],
	"exclude": [
		"node_modules",
		"dist"
	]
}